  "scripts": {
    "build:server": "rimraf dist/server && babel ./server --out-dir dist/server --copy-files",
    "build:client": "rimraf dist/client && webpack --config ./server/webpack.prod.js",
    "start:server:dev": "set NODE_ENV=development&& npm run build:server && node dist/server/index.js",
    "start:server": "set NODE_ENV=production&& npm run build:server && node dist/server/index.js",
    "start": "npm-run-all --parallel start:server build:client"
